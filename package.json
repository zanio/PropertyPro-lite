{
  "name": "api",
  "version": "1.0.0",
  "description": "PropertyPro-lite enpoint to be consumed by my frontend",
  "main": "src/app.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --require @babel/register --require @babel/polyfill -r dotenv/config src/test/*.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "babel-node docs.js",
    "dev": "set DEBUG=http & nodemon --exec babel-node src/app.js ",
    "clean": "rm -rf dist && mkdir dist",
    "script": "babel-node src/services/test.js",
    "build-babel": "npm run clean && babel src --out-dir dist",
    "start": "npm run serve",
    "postgres": "node db createUserTable & node db creatAdminTable & node db createPropertyTable & node db createReportTable & node db createFlaggedTable",
    "serve": "node dist/app.js",
    "psql": "psql -h localhost -p 2000 -U postgres -W"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanio/PropertyPro-lite.git"
  },
  "author": "Aniefiok Akpan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zanio/PropertyPro-lite/issues"
  },
  "homepage": "https://github.com/zanio/PropertyPro-lite#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.4",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.2.1",
    "nyc": "^14.1.1",
    "open": "^6.4.0",
    "pg": "^7.11.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "swagger-editor": "^3.6.31",
    "swagger-ui-express": "^4.0.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "express-swagger-generator": "^1.1.15",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1"
  }
}
