{
  "name": "api",
  "version": "1.0.0",
  "description": "PropertyPro-lite enpoint to be consumed by my frontend",
  "main": "src/app.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --require @babel/register --require @babel/polyfill src/test/*.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon --exec babel-node src/app.js",
    "clean": "rm -rf dist && mkdir dist",
    "build-babel": "npm run clean && babel src --out-dir dist",
    "start":"npm run serve",
    "serve": "node dist/app.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanio/PropertyPro-lite.git"
  },
  "author": "Aniefiok Akpan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zanio/PropertyPro-lite/issues"
  },
  "homepage": "https://github.com/zanio/PropertyPro-lite#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.14.0",
    "datauri": "^2.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "multer": "^1.4.1",
    "nyc": "^14.1.1",
    "swagger-editor": "^3.6.31",
    "swagger-ui-express": "^4.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "express-swagger-generator": "^1.1.15",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1"
  }
}
